{
  "name": "@sentryai/sdk",
  "version": "0.1.0",
  "description": "JavaScript/TypeScript SDK for Sentry for AI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest"
  },
  "keywords": [
    "sentry",
    "ai",
    "llm",
    "observability",
    "monitoring"
  ],
  "author": "SentryAI Team",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
